@model IEnumerable<SimpleInjectorExample.Domain.Entities.Product>

@{
    ViewBag.Title = "Index";
}

<h2>Product</h2>
<div>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Name Product</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(x => item.Name)</td>
                </tr>
            }
        </tbody>
    </table>
</div>